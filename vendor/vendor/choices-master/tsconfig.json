{
  "compilerOptions": {
    "baseUrl": "/",
    "outDir": "./dist/",
    "allowJs": true,
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es2017", "dom"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "types": ["cypress"]
  },
  "include": ["./src/**/*", "./cypress/**/*"]
}
